{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://Q-note.virts.app",
  "description": "书卷多情似故人，晨昏忧乐每相亲。眼前直下三千字，胸次全无一点尘。",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
